{
  "name": "ts-starter",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun index.ts",
    "build": "bun build --compile index.ts --outfile dist/create-ts",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "knip": "knip",
    "typecheck": "tsgo --noEmit"
  },
  "dependencies": {
    "clack-machine": "latest"
  },
  "devDependencies": {
    "@biomejs/biome": "latest",
    "@types/bun": "latest",
    "@typescript/native-preview": "latest",
    "biome-plugin-no-type-assertion": "latest",
    "knip": "latest"
  }
}
